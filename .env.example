# Gmail configuration
EMAIL_USER=your-email@gmail.com
# Replace with your 16-character App Password (remove any spaces)
# Example: if Google gives you "abcd efgh ijkl mnop"
# Write it as: EMAIL_PASSWORD=abcdefghijklmnop
EMAIL_PASSWORD=your-app-password-here
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587

# Docker Configuration
COMPOSE_PROJECT_NAME=system-monitoring

# Dashboard Configuration
DASHBOARD_PORT=5000
MAX_METRICS_HISTORY=100

# Metrics Agent Configuration
METRICS_COLLECTION_INTERVAL=5  # seconds
DEBUG_MODE=False 